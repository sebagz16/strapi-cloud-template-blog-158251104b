      "type": "dynamiczone",
      "components": [
        "value.core-value"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "key_objectives": {
      "type": "dynamiczone",
      "components": [
        "key-objective.key-objective"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "title": {
      "type": "string",
      "required": true,
      "default": "About me",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
